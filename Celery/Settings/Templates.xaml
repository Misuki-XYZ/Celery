<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Celery.Settings"
                    xmlns:view="clr-namespace:Celery.Settings.View">
    <DataTemplate DataType="{x:Type local:BooleanSetting}">
        <view:BooleanSettingView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:ChoiceSetting}">
        <view:ChoiceSettingView/>
    </DataTemplate>
</ResourceDictionary>